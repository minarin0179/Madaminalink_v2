{
    "scripts": {
        "dev": "bun src/bot.ts",
        "start": "bun src/index.ts",
        "daemon": "bun pm2 --no-daemon start Madaminalink_pm2.config.js",
        "format": "bunx --bun biome check --write ./src",
        "docs:dev": "vitepress dev docs --host",
        "docs:build": "bun scripts/generate-releases.mjs && vitepress build docs && bun docs/.vitepress/generateOgImages.mts",
        "docs:preview": "vitepress preview docs",
        "docs:tunnel": "bash scripts/docs-tunnel.sh",
        "releases:generate": "bun scripts/generate-releases.mjs"
    },
    "dependencies": {
        "agenda": "^4.4.0",
        "discord.js": "^14.25.0",
        "dotenv": "^16.6.1",
        "mongoose": "^8.21.0",
        "pm2": "^6.0.14"
    },
    "devDependencies": {
        "@biomejs/biome": "2.3.11",
        "@types/bun": "^1.3.5",
        "@types/mongoose": "^5.11.97",
        "@vercel/og": "^0.8.5",
        "glob": "^13.0.0",
        "gray-matter": "^4.0.3",
        "typescript": "^5.1.6",
        "vitepress": "^1.6.4"
    },
    "name": "madaminalink_v2",
    "module": "src/index.ts",
    "type": "module",
    "private": true
}
